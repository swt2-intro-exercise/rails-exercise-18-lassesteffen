<table id="authors">
  <tr>
    <th>Name</th>
    <th>Homepage</th>
  </tr>
  <% @authors.each do |author| %>
    <tr>
      <td><%= link_to author.name, author_path(author) %></td>
      <td><%= author.homepage %></td>
    </tr>
  <% end %>
</table><br>

<%= link_to 'Create new author', new_author_path %><br>
<%= link_to 'Back', root_path %>